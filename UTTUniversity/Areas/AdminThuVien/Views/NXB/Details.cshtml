@model UTTUniversity.Models.tblNhaXuatBan

<div class="modal-content">
    <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle" style="color:red">Thông tin chi tiết</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-lg-6">
                <div class="form-group">
                    <label>Mã nhà xuất bản: <span class="label-required"></span></label>
                    @Model.MA_NXB
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label>Tên nhà xuất bản: <span class="label-required"></span></label>
                    @Model.TEN_NXB
                </div>
            </div>

            <div class="col-lg-6">
                <div class="form-group">
                    <label>Số điện thoại <span class="label-required"></span></label>
                    @Model.SO_DTHOAI
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label>Địa chỉ: <span class="label-required"></span></label>
                    @Model.DIA_CHI
                </div>
            </div>
            <div class="col-lg-6">
                <label>Mô tả: <span class="label-required"></span></label>
                @Model.GHI_CHU
            </div>

        </div>
    </div>
</div>
