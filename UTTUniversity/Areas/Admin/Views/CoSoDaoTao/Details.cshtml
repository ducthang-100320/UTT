@model UTTUniversity.Models.tblCoSo

<div class="modal-content">
    <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Thông Tin Cơ Sở Đào Tạo</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-lg-6">
                <div class="form-group">
                    <label>Mã Cơ Sở:  <span class="label-required"></span></label>
                    @Model.MA_COSO
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label>Tên Cơ Sở:  <span class="label-required"></span></label>
                    @Model.TEN_COSO
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label>Số Điện Thoại:  <span class="label-required"></span></label>
                    @Model.SO_DTHOAI
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label>Địa Chỉ: <span class="label-required"></span></label>
                    @Model.DIA_CHI
                </div>
            </div>

        </div>
    </div>
</div>
